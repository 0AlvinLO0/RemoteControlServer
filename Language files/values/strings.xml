    <string name="upgrade_code_get_title">About unlock codes</string>
    <string name="upgrade_code_get_desc">You can unlock the premium features for free by activating unlock codes.\n\nSome codes are only valid for a limited time, some won\'t expire. Trial codes can only be activated once.</string>
    <string name="upgrade_iap_get_title">About paid upgrades</string>
    <string name="upgrade_iap_get_desc">Purchased upgrades won\'t expire and will be unlocked on all your devices.\n\nAny upgrade will also remove ads.</string>

    <string name="upgrade_code_facebook_title">Facebook</string>
    <string name="upgrade_code_facebook_description">You can get a free trial code from our Facebook page to get you started.\n\nTo receive your code, navigate your PC\'s browser to:\nhttp://code.android-remote.com</string>
    <string name="upgrade_code_google_title">Google+</string>
    <string name="upgrade_code_google_description">If you sign up as a beta tester on Google+, you will also get a free trial code.\n\nTo receive your code, navigate your PC\'s browser to:\nhttp://beta.android-remote.com</string>
    <string name="upgrade_code_ad_title">Sponsored video</string>
    <string name="upgrade_code_ad_description">Watching an ad will unlock all premium remotes for one session.\n\nThis option may won\'t be available every time.</string>
    <string name="upgrade_code_translate_title">Translate</string>
    <string name="upgrade_code_translate_description">Help us translating this app to your native language and we\'ll reward you with an unlimiited code for your efforts.\n\nDrop us a mail to learn more: contact@android-remote.com</string>

    <string name="upgrade_trial_activated">Trial code has been activated.</string>
    <string name="upgrade_pro_activated">Pro version has been activated.</string>

    <string name="upgrade_remote_locked">Remote locked</string>
    <string name="upgrade_remote_locked_desc">This remote is a pro feature. You can watch an ad, activate an unlock code or purchase an upgrade to use it.</string>
    <string name="upgrade_remote_locked_pirate">Not for pirates</string>
    <string name="upgrade_remote_locked_pirate_desc">This remote has been locked because you are not using an official app version.</string>

